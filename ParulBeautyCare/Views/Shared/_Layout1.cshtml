<script>
    $(document).ready(function () {
          // Remove 'bg-gradient-primary' class from all <a> elements
                $('a.nav-link').removeClass('bg-gradient-primary');
        // Retrieve the current URL
        var currentUrl = window.location.href;

        // Find the matching <a> element based on current URL or anchor href
        var $matchedElement = $('a.nav-link[href="' + currentUrl + '"]');

        if ($matchedElement.length === 0) {
            $matchedElement = $('a.nav-link').filter(function () {
                return this.href === currentUrl || this.href === currentUrl + '/';
            });
        }

        // Add the active class to the matched <a> element
        $matchedElement.addClass('bg-gradient-primary');

        // Add click event handler to all <a> elements
        $('a.nav-link').click(function (e) {
            // Remove 'bg-gradient-primary' class from all <a> elements
            $('a.nav-link').removeClass('bg-gradient-primary');

            // Add 'bg-gradient-primary' class to the clicked <a> element
            $(this).addClass('bg-gradient-primary');

            // Get the href attribute of the clicked <a> element
            var href = $(this).attr('href');

            // Redirect to the link location
            window.location.href = href;
        });
    });

</script>
